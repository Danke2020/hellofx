      fx:controller="org.openjfx.FXMLController">
    <children>
        <HBox alignment="CENTER" layoutX="313.0" layoutY="57.0" prefHeight="57.0" prefWidth="267.0" spacing="25.0">
            <children>
                <TextField fx:id="key" prefHeight="30.0" prefWidth="80.0" promptText="color"/>
                <TextField fx:id="name" prefHeight="30.0" prefWidth="85.0" promptText="Japanese"/>
