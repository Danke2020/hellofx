                <TableColumn fx:id="colorKey" prefWidth="75.0" text="color"/>
                <TableColumn fx:id="colorName" prefWidth="125.0" text="color(Japanese)"/>
                <TableColumn fx:id="imageUrl" prefWidth="75.0" text="image"/>
            </columns>
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </TableView>
        <TextField fx:id="standardPrice" layoutX="62.0" layoutY="24.0" prefHeight="30.0" prefWidth="85.0"
                   promptText="standardPrice" text="4999"/>
        <TextField fx:id="folder" layoutX="320.0" layoutY="24.0" promptText="folder" text="chenshan"/>
        <TextField fx:id="brandName" layoutX="169.0" layoutY="24.0" prefHeight="30.0" prefWidth="93.0"
                   promptText="brandName" text="test"/>
   </children>
</Pane>
